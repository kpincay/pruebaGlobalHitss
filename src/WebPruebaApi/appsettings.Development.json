{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=192.168.100.5;Initial Catalog=prueba; User ID=sa;Password=Admin123456"
    //181.39.148.214 ip publica
  },
  "ConsumoApis": {
    "UrlBaseApiAuth": "http://200.110.64.17:35206/api/v1/"
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "Logs/webapienrolapp-log-.txt",
          "rollingInterval": "Day",
          "outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm} {CorrelationId} [{Level:u3}] {Message:lj}{NewLine}{Exception} {Properties:j}",
          "shared": true
        }
      }

    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ],
    "Properties": {
      "Application": "ApiPrueba"
    }
  }
}
